ct, { useState, useEffect } from \"react\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport Spinner from \"../../components/spinner/Spinner\";\nimport Alert from \"../../components/alert/Alert\";\nimport { createMessage } from \"../../redux/actions/massageAction\";\nimport { Field, reduxForm, reset } from \"redux-form\";\n\nimport \"./contact.style.scss\";\nimport Reseau from \"../../components/reseau/Reseau\";\nimport BaniereContact from \"../../components/banniereContact/BaniereContact\";\nimport TitreSecondary from \"../../components/titre/TitreSecondary\";\n//_________________________________render input______________________________________\n\nconst renderError = (meta) => {\n    if (meta.error && meta.touched) {\n        return <small className=\"errorMess\">{meta.error}</small>;\n    }\n};\n\nconst renderInput = (formProps) => {\n    return (\n        <div className=\"form__group\">\n            <input\n                {...formProps.input}\n                autoComplete=\"off\"\n                className={`${\n                    formProps.meta.touched\n                        ? formProps.meta.error\n                            ? \"inpuTError\"\n                            : \"inputOK\"\n                        : \"\"\n                }  form__input `}\n                placeholder={formProps.placeholder}\n            />\n            <label htmlFor=\"nom\" className=\"form__label\">\n                {formProps.label}\n            </label>\n            {renderError(formProps.meta)}\n        </div>\n    );\n};\n\nconst renderTextarea = (formProps) => {\n    console.log(\n        \"üöÄ ~ file: Contact.jsx ~ line 58 ~ renderTextarea ~ formProps\",\n        formProps.meta\n    );\n    return (\n        <div className=\"form__group\">\n            <textarea\n                name=\"message\"\n                {...formProps.input}\n                autoComplete=\"off\"\n                value={formProps.input.value}\n                onChange={formProps.input.onChange}\n                id=\"message\"\n                cols=\"30\"\n                rows=\"10\"\n                className={`${\n                    formProps.meta.touched\n                        ? formProps.meta.error\n                            ? \"inpuTError\"\n                            : \"inputOK\"\n                        : \"\"\n                }  form__input `}\n                placeholder=\"votre message...\"\n            ></textarea>\n            <label htmlFor=\"massage\" className=\"form__label\">\n                {formProps.label}\n            </label>\n            {renderError(formProps.meta)}\n        </div>\n    );\n};\n\nconst Contact = ({ history, handleSubmit }) => {\n    const [successMess, setSuccessMess] = useState(\n        \"Votre message a √©t√© envoyer avec success!  j'y repondrai des que possible ! merci\"\n    );\n\n    const dispatch = useDispatch();\n\n    const newMessage = useSelector((state) => state.messages);\n    const { loading, success } = newMessage;\n\n    // const form = useSelector((state) => state.form);\n\n    useEffect(() => {\n        if (success) {\n            history.push(\"/contact\");\n        }\n    }, [history, success]);\n\n    //_________________________________fonction______________________________________\n    const onHandleSubmit = (formValue) => {\n        const data = {\n            name: formValue.name,\n            email: formValue.email,\n            message: formValue.message,\n        };\n        dispatch(createMessage(data));\n        dispatch(reset(\"messageForm\"));\n    };\n    return (\n        <div className=\"contact\">\n            <TitreSecondary>vous pouvez me contacter</TitreSecondary>\n            <BaniereContact message=\" 06-63-32-38-36\" />\n            <div className=\"spin\">\n                {loading && <Spinner message=\"\" />}\n                {/* {error && <Alert variant=\"danger\">{error}</Alert>} */}\n                {success && <Alert variant=\"success\">{successMess}</Alert>}\n            </div>\n\n            <div className=\"formulaire\">\n                <div className=\"formulaire__form\">\n                    <h4>Laisser moi un message</h4>\n                    <form\n                        onSubmit={handleSubmit(onHandleSubmit)}\n                        className=\"form\"\n                    >\n                        <Field\n                            name=\"name\"\n                            component={renderInput}\n                            label=\"Votre nom !\"\n                            placeholder=\"votre nom...\"\n                        />\n\n                        <Field\n                            name=\"email\"\n                            component={renderInput}\n                            label=\"Votre mail !\"\n                            placeholder=\"votre email...\"\n                        />\n\n                        <Field\n                            name=\"message\"\n                            component={renderTextarea}\n                            label=\"Votre Message !\"\n                        />\n\n                        <button className=\"btn-submit\" type=\"submit\">\n                            <i className=\"fas fa-arrow-right\"></i>\n                            envoyer\n                        </button>\n                    </form>\n                </div>\n            </div>\n            <hr />\n\n            <div>\n                <Reseau />\n            </div>\n        </div>\n    );\n};\n\nconst validate = (formValues) => {\n    const errors = {};\n    if (!formValues.name) {\n        //only ran if user did not enter a name\n        errors.name = \"vous devez entrer un nom !\";\n    }\n\n    if (\n        /^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\\.[a-zA-Z0-9-]+)*$/.test(\n            formValues.email\n        ) !== true\n    ) {\n        errors.email = \"votre adresse email a un probl√©mes !!!\";\n    }\n    if (!formValues.email) {\n        //only ran if user did not enter a name\n        errors.email = \"vous devez entrer votre mail !\";\n    }\n\n    if (!formValues.message) {\n        //only ran if user did not enter a name\n        errors.message = \"vous devez entrer un message !\";\n    }\n\n    return errors;\n};\n\nexport default reduxForm({\n    form: \"messageForm\",\n    validate,\n})(Contact);\n","/media/ni/Êñ∞Âä†Âç∑/mern-stack/portfolio/frontend/src/redux/type/messageType.js",[],"/media/ni/Êñ∞Âä†Âç∑/mern-stack/portfolio/frontend/src/redux/actions/massageAction.js",[],"/media/ni/Êñ∞Âä†Âç∑/mern-stack/portfolio/frontend/src/components/banniereContact/BaniereContact.jsx",[],"/media/ni/Êñ∞Âä†Âç∑/mern-stack/portfolio/frontend/src/components/CometenceService/CompetenceService.jsx",[],"/media/ni/Êñ∞Âä†Âç∑/mern-stack/portfolio/frontend/src/components/compService/CompService.jsx",[],"/media/ni/Êñ∞Âä†Âç∑/mern-stack/portfolio/frontend/src/components/spinner/Spinner.jsx",[],"/media/ni/Êñ∞Âä†Âç∑/mern-stack/portfolio/frontend/src/components/alert/Alert.jsx",[],"/media/ni/Êñ∞Âä†Âç∑/mern-stack/portfolio/frontend/src/components/reseau/Reseau.jsx",["183","184","185","186"],"import React, { useState } from 'react';\nimport {Link} from 'react-router-dom';\nimport logoGit from '../../img/gitbb.png';\nimport logoLink from '../../img/link.png'\n\nconst Reseau = () => {\n\n\n    const styleLogo = {\n        width: '180px',\n        height: '180px',\n\n    };\n\n\n    return (\n\n        <>\n\n            <div className= \"container P-2 col-12 col-md-6\">\n                <h2 className=\"text-center\">Mes r√©seaux pro : </h2>\n\n                <div className=\"d-flex row justify-content-around m-4 p-4\">\n                <a target=\"_blank\" href=\"https://www.linkedin.com/in/nicolas-riot\"><img src={logoLink} alt=\"logo\" style={styleLogo} /></a>\n                <a target=\"_blank\" href=\"https://github.com/nicoWeb31\"><img src={logoGit} alt=\"logo\" style={styleLogo} /></a>\n\n                </div>\n            </div>\n\n\n